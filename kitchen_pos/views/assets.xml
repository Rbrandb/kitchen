<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <template id="assets_backend" inherit_id="web.assets_backend">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" href="/kitchen_pos/static/src/scss/list_view.scss"/>
                <link rel="stylesheet" href="https://cdn.datatables.net/1.11.3/css/dataTables.bootstrap4.min.css"/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"/>
                <script type="text/javascript" src="https://cdn.datatables.net/1.11.3/js/dataTables.bootstrap4.min.js"/>
                <script type="text/javascript" src="/kitchen_pos/static/src/js/list_reload.js"/>
                <script type="text/javascript" src="/kitchen_pos/static/src/js/kitchen_notify.js"/>
            </xpath>
        </template>

        <template id="kitchen_pos" inherit_id="point_of_sale.assets">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" href="/kitchen_pos/static/src/scss/kitchen_pos.scss"/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/kitchen_pos/static/src/js/pos_combo.js"/>
                <script type="text/javascript" src="/kitchen_pos/static/src/js/SelectComboProductPopupWidget.js"/>
                <script type="text/javascript" src="/kitchen_pos/static/src/js/pos_db.js"/>
                <script type="text/javascript" src="/kitchen_pos/static/src/js/pos_screen.js"/>
                <script type="text/javascript" src="/kitchen_pos/static/src/js/order_kitchen_button.js"/>
                <script type="text/javascript" src="/kitchen_pos/static/src/js/pos_order_removal.js"/>
            </xpath>
        </template>

    </data>
</odoo>